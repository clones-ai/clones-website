{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "1.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "lighthouse": "node scripts/lighthouse-audit.js",
    "bundle:analyze": "npm run build && node scripts/bundle-analyzer.js",
    "images:analyze": "node scripts/optimize-images.js",
    "images:convert": "node scripts/convert-to-webp.js",
    "audit:full": "npm run build && npm run preview & sleep 3 && npm run lighthouse && kill %1",
    "audit:complete": "npm run bundle:analyze && npm run images:analyze && echo '\nðŸŽ¯ Complete audit finished!'",
    "optimize:images": "npm run images:convert && npm run images:analyze",
    "perf:save": "node scripts/performance-comparison.js save",
    "perf:compare": "node scripts/performance-comparison.js compare",
    "perf:current": "node scripts/performance-comparison.js current",
    "docker:test": "./scripts/test-docker.sh",
    "docker:dev": "docker compose --profile dev up",
    "docker:prod": "docker compose --profile prod up",
    "start": "node server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "deploy": "npm run build && gh-pages -d dist",
    "predeploy": "npm run build"
  },
  "dependencies": {
    "@rainbow-me/rainbowkit": "^2.2.8",
    "@splinetool/react-spline": "^4.1.0",
    "@splinetool/runtime": "^1.10.57",
    "@splinetool/viewer": "^1.10.57",
    "@tanstack/react-query": "^5.59.0",
    "express": "^4.18.2",
    "framer-motion": "^12.23.12",
    "lenis": "^1.3.11",
    "lucide-react": "^0.344.0",
    "pino": "^8.19.0",
    "pino-http": "^9.0.0",
    "pino-pretty": "^11.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.7.1",
    "siwe": "^3.0.0",
    "uuid": "^9.0.1",
    "viem": "^2.38.0",
    "wagmi": "^2.17.5",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@fullhuman/postcss-purgecss": "^7.0.2",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "chrome-launcher": "^1.2.0",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "lighthouse": "^12.8.2",
    "postcss": "^8.4.35",
    "sharp": "^0.34.3",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.4.3",
    "typescript": "5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^7.1.4"
  }
}